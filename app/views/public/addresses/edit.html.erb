<div class="container">
<h4>配送先編集</h3>
</div>
</div>

<%= form_with model: @address, local:true do |f| %>

<table class="table-light table">
<tbody>
<tr>
<th>郵便番号(ハイフンなし)</th>
<td> <%= f.text_field :postcode %></td>
</tr>
<tr>
<th>住所</th>
<td> <%= f.text_field :address, :size =>"60" %></td>
</tr>
<tr>
<th>宛名</th>
<td><%= f.text_field :name %></td>
<td><%= f.submit '変更内容を保存', class:"btn btn-primary" %></td>
</tr>
</tbody>
</table>
<% end %>
</div>