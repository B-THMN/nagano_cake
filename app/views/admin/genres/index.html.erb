<h1>ジャンル・一覧追加</h1>

<tr>
  <%= form_with model: @genre,url: admin_genres_path,local:true do |f| %>
    <div>
     <%= f.label :ジャンル名 %>
     <%= f.text_field :name, class: 'form-control name' %>
     <%= f.submit '新規登録', class: 'btn btn-success' %>
    </div>
  <% end %>
</tr>

<% @genres.each do |genre| %>
<table>
<tr>
<td><%= genre.name %></td>
<td><%= link_to '編集する',edit_admin_genre_path(genre), class: 'btn btn-success' %></td>
</tr>
</table>
<% end %>
